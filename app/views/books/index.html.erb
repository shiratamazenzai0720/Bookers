    <span><%= book.title %></span>
    <p>Body</p>
    <span><%= book.body %>
    <%= link_to "Show", book_path(book.id) %>
    <%= link_to "Edit", edit_book_path(book.id) %>
    <%= link_to "Destroy", book_path(book.id), method: :delete, "data-confirm" => "Are you sure ï¼Ÿ" %>
    </span>
  <% end %>